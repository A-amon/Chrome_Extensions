{
    "manifest_version": 2,
    "name": "Coronavirus Blocker",
    "version": "0.1.0",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["styles.css"]
      }
    ],
    "browser_action": {
        "default_icon": "logo.png"
      },
      "icons": {
        "128": "logo.png"
      }
  }